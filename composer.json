{
    "description": "Umbrella Admin demo app",
    "name": "umbrella2/umbrella-demo",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "league/flysystem-bundle": "^2.0",
        "liip/imagine-bundle": "^2.6",
        "sensio/framework-extra-bundle": "^5.5",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/asset": "5.2.*",
        "symfony/config": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/lock": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "5.2.*",
        "symfony/rate-limiter": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "5.2.*",
        "umbrella2/adminbundle": "dev-master",
        "umbrella2/corebundle": "dev-master"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^0.4.0",
        "phpstan/phpstan": "^0.12.83",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.25",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.2.*"
        }
    }
}
