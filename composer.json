{
    "description": "Umbrella Admin demo app",
    "name": "umbrella2/umbrella-demo",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "2.9",
        "knplabs/knp-time-bundle": "^1.16",
        "phpdocumentor/reflection-docblock": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/asset": "^5.3|^6.0",
        "symfony/config": "^5.3|^6.0",
        "symfony/console": "^5.3|^6.0",
        "symfony/dotenv": "^5.3|^6.0",
        "symfony/error-handler": "^5.3|^6.0",
        "symfony/expression-language": "^5.3|^6.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.3|^6.0",
        "symfony/framework-bundle": "^5.3|^6.0",
        "symfony/http-client": "^5.3|^6.0",
        "symfony/intl": "^5.3|^6.0",
        "symfony/lock": "^5.3|^6.0",
        "symfony/mailer": "^5.3|^6.0",
        "symfony/messenger": "^5.3|^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^5.3|^6.0",
        "symfony/property-access": "^5.3|^6.0",
        "symfony/property-info": "^5.3|^6.0",
        "symfony/proxy-manager-bridge": "^5.3|^6.0",
        "symfony/rate-limiter": "^5.3|^6.0",
        "symfony/security-bundle": "^5.3|^6.0",
        "symfony/serializer": "^5.3|^6.0",
        "symfony/string": "^5.3|^6.0",
        "symfony/translation": "^5.3|^6.0",
        "symfony/twig-bundle": "^5.3|^6.0",
        "symfony/validator": "^5.3|^6.0",
        "symfony/web-link": "^5.3|^6.0",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.3|^6.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "umbrella2/adminbundle": "4.x-dev",
        "umbrella2/corebundle": "4.x-dev",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^0.4.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.83",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.25",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.3|^6.0",
        "symfony/css-selector": "^5.3|^6.0",
        "symfony/debug-bundle": "^5.3|^6.0",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.3|^6.0",
        "symfony/web-profiler-bundle": "^5.3|^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^5.3|^6.0"
        }
    }
}
